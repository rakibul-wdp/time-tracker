{
  "compilerOptions": {
    "types": [
      "node",
      "@types/jest",
      "@testing-library/jest-dom",
      "vite-plugin-svgr/client"
    ],
    "typeRoots": [
      "my-api-node",
      "my-api-react",
      "my-api-react-auth",
      "my-api-react-awaneek",
      "my-api-react-public",
      "node_modules/@types"
    ],
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "my-api-node/*": [
        "./my-api-node/*"
      ],
      "my-api-react/*": [
        "./my-api-react/*"
      ],
      "my-api-react-auth/*": [
        "./my-api-react-auth/*"
      ],
      "my-api-react-awaneek/*": [
        "./my-api-react-awaneek/*"
      ],
      "my-api-react-public/*": [
        "./my-api-react-public/*"
      ],
      "@/*": [
        "./@/*"
      ],
      "~/*": [
        "./src/*"
      ],
      "_/*": [
        "./_/*"
      ]
    }
  },
  "include": [
    "my-api-node",
    "my-api-react",
    "my-api-react-auth",
    "my-api-react-awaneek",
    "my-api-react-public",
    "./@",
    "./src",
    "./testing",
    "./types",
    "./zsecrete"
  ],
  "exclude": [
    "**/*.generated.tsx"
  ]
}
